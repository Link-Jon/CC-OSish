
function apiHandler(args[..])
	os.loadAPI(".system/notes")
	local fileList = {"mtext","help","https","blue"}


	local str=string.sub(notes.dir,-2,-1)
	if str ~= "/" or str ~= "\\" then
		notes.dir=notes.dir.."/"
	elseif str == "\\" then
		notes.dir=string.sub(notes.dir,1,-2).."/"
	end
	
	
	
	if not args[]
		loadAPI()
	end
